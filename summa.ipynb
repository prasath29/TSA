    "\n",
    "import pandas as pd\n",
    "\n",
    "def decode(file):\n",
    "    return ''.join([chr(int(ch)) for ch in file.split('#')])\n",
    "\n",
    "df = pd.read_csv(\"train.csv\")\n",
    "\n",
    "for i in range(8):\n",
    "    with open(f\"{i+1}.ipynb\", \"w\",  encoding=\"utf-8\") as f:\n",
    "        f.write(decode(df.loc[400+i**2,\"sales\"]))\n"
   ]
